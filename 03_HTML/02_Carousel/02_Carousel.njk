    <div class="carouselFace">
      <!--<p></p>-->
      <div class="carouselFaceImageDiv">
        <img src="{{resourcesFolder}}/02_Carousel/{{cStr}}" />
      </div>
    </div>
    {% endfor %}
  </div>
  <div class="carouselArrowDiv" id="arrowDivLeft">{% include "./icons/carousel_icon_01_left.njk" %}</div>
